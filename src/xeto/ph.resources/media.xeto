//////////////////////////////////////////////////////////////////////////
// Document
//////////////////////////////////////////////////////////////////////////

// Document resource files for including word processor, spreadsheets,
// and presentations.
Document : Resource {
  document
}

// Plain text document
TextDocument : Document {
  text
}

// Markdown text document
MarkdownDocument : Document {
  markdown
}

// Document in PDF format
PdfDocument : Document {
  pdf
}

// Document in Microsoft Word format
WordDocument : Document {
  word
}

// Spreadsheet in Microsoft Excel format
ExcelDocument : Document {
  excel
}

// Presentation in Microsoft PowerPoint format
PowerPointDocument : Document {
  powerPoint
}

// Link to a Google doc
GoogleDocument : Document {
  googleDoc
}

// Link to a Google sheets document
GoogleSheetsDocument : Document {
  googleSheets
}

// Link to a Google slides document
GoogleSlidesDocument : Document {
  googleSlides
}

// AutoCAD dwg document
AutoCadDocument : Document {
  googleSlides
}

//////////////////////////////////////////////////////////////////////////
// Image
//////////////////////////////////////////////////////////////////////////

// Image resource file
Image : Resource {
  image
}

// Scalable vector graphics image file
SvgImage : Image {
  svg
}

// Image file in PNG format
PngImage : Image {
  png
}

// Image file in JPEG format
JpegImage : Image {
  jpeg
}

//////////////////////////////////////////////////////////////////////////
// Audio
//////////////////////////////////////////////////////////////////////////

// Audio resource file
Audio : Resource {
  audio
}
//////////////////////////////////////////////////////////////////////////
// Video
//////////////////////////////////////////////////////////////////////////

// Video resource file
Video : Resource {
  video
}

//////////////////////////////////////////////////////////////////////////
// Misc
//////////////////////////////////////////////////////////////////////////

// Zip archive file
Zip : Resource {
  zip
}





